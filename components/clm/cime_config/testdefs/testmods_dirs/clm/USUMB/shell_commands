# shell commands to execute xmlchange commands written by PTCLMmkdata:
# ./PTCLMmkdata -s US-UMB -d /glade/p/cesmdata/cseg/inputdata --mydatadir /glade/p/cesmdata/cseg/inputdata/lnd/clm2/PTCLMmydatafiles.c160208 --phys=clm4_5 
./xmlchange CLM_USRDAT_NAME=1x1pt_US-UMB
./xmlchange DATM_CLMNCEP_YR_START=1999
./xmlchange DATM_CLMNCEP_YR_END=2006
./xmlchange CLM_BLDNML_OPTS='-mask navy'
./xmlchange MPILIB=mpi-serial
./xmlchange ATM_DOMAIN_PATH='$DIN_LOC_ROOT/lnd/clm2/PTCLMmydatafiles.c160208/1x1pt_US-UMB'
./xmlchange LND_DOMAIN_PATH='$DIN_LOC_ROOT/lnd/clm2/PTCLMmydatafiles.c160208/1x1pt_US-UMB'
./xmlchange ATM_DOMAIN_FILE=domain.lnd.1x1pt_US-UMB_navy.160208.nc
./xmlchange LND_DOMAIN_FILE=domain.lnd.1x1pt_US-UMB_navy.160208.nc
./xmlchange CALENDAR=GREGORIAN
./xmlchange DOUT_S=FALSE
./xmlchange ATM_NCPL=24
./xmlchange RUN_STARTDATE=1999-01-01
./xmlchange DATM_CLMNCEP_YR_ALIGN=1999
./xmlchange DIN_LOC_ROOT_CLMFORC='$DIN_LOC_ROOT/lnd/clm2/PTCLMmydatafiles.c160208'
